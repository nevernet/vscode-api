// 测试重复定义检查修复

[[card全局参数配置]]
apilist "cardconfig" {
  input struct {
    id int
    name string
  }
  output struct {
    result bool
  }
}

// 这应该不报重复定义错误
typedef struct {
  field1 int
  field2 string
} MyStruct

typedef struct {
  field1 int    // 这个在不同struct中，不应该报错
  field3 bool
} AnotherStruct

// 这应该报重复定义错误（同一struct内字段重复）
typedef struct {
  field1 int
  field1 string  // 重复字段，应该报错
} BadStruct

// 这应该报重复定义错误（同一enum内值重复）  
typedef enum {
  VALUE1
  VALUE2
  VALUE1         // 重复枚举值，应该报错
} BadEnum

// 这应该不报错（不同enum中的相同值名）
typedef enum {
  VALUE1
  VALUE2
} Enum1

typedef enum {
  VALUE1         // 不同enum中，不应该报错
  VALUE3
} Enum2