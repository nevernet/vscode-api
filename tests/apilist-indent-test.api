// 测试 apilist 内 api 缩进修复

[[企业的社会化信息]]
apilist "cardsocialinfo" {
  [[]]
  api "addoredit" {
    input struct {
      field1 int
      field2 string
    }
    output struct {
      result bool
    }
  }

  api "delete" {
    input struct {
      id int
    }
    output struct {
      success bool
    }
  }
}

// 顶层 api 应该不缩进
api "standalone" {
  input struct {
    data string
  }
  output struct {
    result int
  }
}