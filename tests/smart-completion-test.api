// 测试智能代码补全功能

// 全局定义
typedef struct {
  id           Int
  name         String
  email        String
} UserInfo

typedef struct {
  code         Int
  message      String
  data         UserInfo
} ApiResponse

typedef enum {
  ACTIVE,
  INACTIVE,
  PENDING
} UserStatus

// API定义
api "getUserInfo" {
  input UserInfo  // 这里应该提示UserInfo结构体
  output ApiResponse  // 这里应该提示ApiResponse结构体
}

// 另一个结构体定义
typedef struct {
  // 在这里输入应该提示所有类型: Int, String, UserInfo, ApiResponse等
  userId       Int
  status       UserStatus // 这里应该提示UserStatus枚举
  createdAt    String
} UserProfile

// 测试枚举
typedef enum {
  // 这里应该提示枚举值
  DRAFT,
  CONFIRMED
} OrderStatus

// 测试API列表
apilist "userAPIs" {
  // 这里应该提示API相关关键字
  api "createUser" {
    // 这里应该提示input, output等
    input UserProfile
    output ApiResponse
  }
}